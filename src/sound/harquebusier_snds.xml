<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
  <protounit name="Harquebusier">
    <soundtype name="Select">
      <soundset name="SpanishCrossbowmanSelect"/>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt"/>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath"/>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth"/>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="SpanishCrossbowmanAcknowledge"/>
        </choice>
        <choice name="enemy">
          <soundset name="SpanishCrossbowmanAttack"/>
        </choice>
      </targetlogic>
    </soundtype>
  </protounit>
</protounitsounddef>
